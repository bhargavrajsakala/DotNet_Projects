@model IEnumerable<MVCPro.Models.StudentModel>

<h2>Student List</h2>
<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
        <th>Email</th>
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.StudentId</td>
            <td>@s.Name</td>
            <td>@s.Age</td>
            <td>@s.Email</td>
        </tr>
    }
</table>